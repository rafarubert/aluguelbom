<% form_for([:admin ,@subcategoria]) do |f| %>
<fieldset>
	<%= f.error_messages %>
	<label>Categoria</label>
	<%=f.collection_select :categoria_id, Categoria.all(:order => :descricao), :id, :descricao %>
	<label>Descrição</label>
	<%=f.text_field :descricao, :class => 'text-long'%>
</fieldset>
<%= f.submit 'Salvar' %>
ou
<%= link_to 'Cancelar', admin_subcategorias_path %>
<% end %>
